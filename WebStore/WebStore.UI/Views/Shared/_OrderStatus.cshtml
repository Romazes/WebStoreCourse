@model string

<div style="width:100%">
    @if (Model == StaticDetail.StatusSubmitted)
    {
        <img src="~/images/OrderPlaced.png" width="100%" />
    }
    @if (Model == StaticDetail.StatusInProcess)
    {
        <img src="~/images/InKitchen.png" width="100%" />
    }
    @if (Model == StaticDetail.StatusReady)
    {
        <img src="~/images/ReadyForPickup.png" width="100%" />
    }
    @if (Model == StaticDetail.StatusCompleted)
    {
        <img src="~/images/completed.png" width="100%" />
    }
</div> 